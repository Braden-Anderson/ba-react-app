{
  "expo": {
    "name": "Light Builder",
    "icon": "./path/to/your/app-icon.png",
    "version": "1.0.0",
    "slug": "lightbuilder",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "ios": {
      "bundleIdentifier": "com.baengineering.lightbuilder"
    },
    "android": {
      "package": "com.baengineering.lightbuilder"
    },
    "platforms": [
      "ios",
